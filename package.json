{
  "name": "vecfs-ts",
  "version": "0.2.0",
  "description": "VecFS MCP server: vector storage and search for AI agents via the Model Context Protocol.",
  "main": "dist/mcp-server.js",
  "bin": {
    "vecfs-ts": "dist/mcp-server.js",
    "vecfs-embed-ts": "dist/embed-cli.js"
  },
  "files": [
    "dist/",
    "vecfs-memory/",
    "patches/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "check": "node scripts/inject-version.mjs && tsc --noEmit",
    "build": "node scripts/inject-version.mjs && rm -rf dist && tsc --noEmit && node esbuild.config.mjs",
    "start": "node dist/mcp-server.js",
    "test": "vitest run",
    "test:integration": "vitest run --testTimeout 30000 integration.test.ts",
    "test:http": "npm run build && vitest run --testTimeout 30000 http-integration.test.ts",
    "prepublishOnly": "npm run build",
    "postinstall": "patch-package",
    "test:perf": "vitest run embedder-perf"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WazzaMo/vecfs.git"
  },
  "keywords": [
    "vecfs",
    "vector",
    "sparse-vector",
    "mcp",
    "model-context-protocol",
    "agent-memory",
    "long-term-memory",
    "local-first"
  ],
  "author": "Warwick Molloy",
  "license": "Apache-2.0",
  "type": "module",
  "bugs": {
    "url": "https://github.com/WazzaMo/vecfs/issues"
  },
  "homepage": "https://github.com/WazzaMo/vecfs#readme",
  "dependencies": {
    "@huggingface/transformers": "^3.8.1",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "cors": "^2.8.6",
    "express": "^5.2.1",
    "fastembed": "^2.1.0",
    "yaml": "^2.7.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.3",
    "esbuild": "^0.27.3",
    "patch-package": "^8.0.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "overrides": {
    "tar": "^7.5.8"
  }
}
